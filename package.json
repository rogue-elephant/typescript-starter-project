{
  "name": "typescript-project-generator",
  "version": "0.0.1",
  "description": "Tool for generating typescript projects quickly.",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "rogue-elephant-ts-generator": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo"
  },
  "scripts": {
    "test:watch": "ts-mocha -p ./tsconfig.json ./**/*.test.ts -w --watch-extensions ts",
    "test": "cross-env TS_NODE_FILES=true node_modules/.bin/mocha --exit --require ts-node/register --colors ./**/*.test.ts",
    "coverage": "nyc npm run test",
    "build": "tsc && shx rm -rf dist/templates && shx cp -r src/templates dist",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "start": "ts-node src/index.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/__tests__/**",
      "src/debugging.ts",
      "**/*.js"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "all": true
  },
  "keywords": [],
  "author": "Thomas Randell",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/ejs": "^3.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.30",
    "@types/shelljs": "^0.8.6",
    "@types/yargs": "^15.0.4",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.16",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "ejs": "^3.0.1",
    "inquirer": "^7.1.0",
    "shelljs": "^0.8.5",
    "shx": "^0.3.2",
    "yargs": "^15.3.1"
  }
}
